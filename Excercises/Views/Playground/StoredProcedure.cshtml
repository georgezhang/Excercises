@model Excercises.Models.PlaygroundViewModels

@{
    ViewBag.Title = "StoredProcedure";
}

<h2>Stored Procedure</h2>

<h3>Peple</h3>
@if (Model.people != null)
{
    foreach (var p in Model.people)
    {
        <ul class="list-group">
            <li class="list-group-item">
                @Html.DisplayFor(_ => p.FirstName)
            </li>
        </ul>
    }
}

<h3>Person</h3>
@if (Model.person != null)
{
    <ul class="list-group">
        <li class="list-group-item">
            @Html.DisplayFor(_ => Model.person.FirstName)
        </li>
    </ul>
}
<h3>2 persons</h3>
@if (Model.persons != null)
{
    <p>@Html.DisplayFor(_ => Model.persons.Item1.First().FirstName)</p>
    <p>@Html.DisplayFor(_ => Model.persons.Item2.First().FirstName)</p>
}